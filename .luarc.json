{
  "runtime": {
    "version": "LuaJIT",
    "path": [
      "?.lua",
      "?/init.lua"
    ]
  },
  "diagnostics": {
    "globals": [
      "vim"
    ]
  },
  "workspace": {
    "library": [
      "${3rd}/luv/library",
      "${3rd}/busted/library"
    ],
    "checkThirdParty": false
  },
  "telemetry": {
    "enable": false
  },
  "hint": {
    "enable": true,
    "paramType": true,
    "setType": false,
    "paramName": "Disable",
    "semicolon": "Disable",
    "arrayIndex": "Disable"
  }
}